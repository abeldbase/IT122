<!-- employees.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>All Cars</title>
  </head>
  <body>
    <h1>All cars</h1>
    <ul>
      <% cars.forEach(car => { %>
      <li><a href="/cars/<%= car.id %>"><%= car.brand %></a></li>
      <% }); %>
    </ul>
  </body>
</html>
